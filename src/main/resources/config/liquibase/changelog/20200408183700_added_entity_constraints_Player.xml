<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Player.
    -->
    <changeSet id="20200408183700-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="score_id"
                                 baseTableName="player"
                                 constraintName="fk_player_score_id"
                                 referencedColumnNames="id"
                                 referencedTableName="score"/>

        <addForeignKeyConstraint baseColumnNames="party_id"
                                 baseTableName="player"
                                 constraintName="fk_player_party_id"
                                 referencedColumnNames="id"
                                 referencedTableName="party"/>

    </changeSet>
</databaseChangeLog>
