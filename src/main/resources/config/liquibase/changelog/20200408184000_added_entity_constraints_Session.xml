<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Session.
    -->
    <changeSet id="20200408184000-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="session_id"
                                 baseTableName="session_players"
                                 constraintName="fk_session_players_session_id"
                                 referencedColumnNames="id"
                                 referencedTableName="session"/>

        <addForeignKeyConstraint baseColumnNames="players_id"
                                 baseTableName="session_players"
                                 constraintName="fk_session_players_players_id"
                                 referencedColumnNames="id"
                                 referencedTableName="player"/>
        
        <addForeignKeyConstraint baseColumnNames="keywords_id"
                                 baseTableName="session"
                                 constraintName="fk_session_keywords_id"
                                 referencedColumnNames="id"
                                 referencedTableName="keyword"/>

        <addForeignKeyConstraint baseColumnNames="tags_id"
                                 baseTableName="session"
                                 constraintName="fk_session_tags_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tag"/>

        <addForeignKeyConstraint baseColumnNames="optional_images_id"
                                 baseTableName="session"
                                 constraintName="fk_session_optional_images_id"
                                 referencedColumnNames="id"
                                 referencedTableName="optional_image"/>

        <addForeignKeyConstraint baseColumnNames="actors_id"
                                 baseTableName="session"
                                 constraintName="fk_session_actors_id"
                                 referencedColumnNames="id"
                                 referencedTableName="actor"/>

        <addForeignKeyConstraint baseColumnNames="answers_id"
                                 baseTableName="session"
                                 constraintName="fk_session_answers_id"
                                 referencedColumnNames="id"
                                 referencedTableName="answer"/>

    </changeSet>
</databaseChangeLog>
